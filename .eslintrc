{
    "rules": {
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "no-console":[
            0
        ]
    },
    "globals": {
        "chrome": true,
        "describe": true,
        "it": true,
        "beforeEach": true,
        "define": true
    },
    "env": {
        "es6": true,
        "browser": true,
        "commonjs": true
    },
    "extends": "eslint:recommended"
}
